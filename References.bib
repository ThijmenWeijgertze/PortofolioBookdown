  shorttitle = {Exploring {{Metabolic Anomalies}} in {{COVID-19}} and {{Post-COVID-19}}},
  author = {{Oropeza-Valdez}, Juan Jos{\'e} and {Padron-Manrique}, Cristian and {V{\'a}zquez-Jim{\'e}nez}, Aar{\'o}n and Soberon, Xavier and {Resendis-Antonio}, Osbaldo},
  year = {2024},
  month = apr,
  primaryclass = {New Results},
  pages = {2024.04.15.589583},
