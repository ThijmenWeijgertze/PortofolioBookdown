@article{arredondo-alonsoGplasComprehensiveTool2020,
  title = {Gplas: A Comprehensive Tool for Plasmid Analysis Using Short-Read Graphs},
  shorttitle = {Gplas},
  author = {{Arredondo-Alonso}, Sergio and Bootsma, Martin and Hein, Ya{\"i}r and Rogers, Malbert R C and Corander, Jukka and Willems, Rob J L and Sch{\"u}rch, Anita C},
